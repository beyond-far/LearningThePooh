{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.2.4",
  "default_locale": "zh_CN",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "browser_action": {
    "default_icon": "img/16.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/init.js"
      ],
      "matches": [
        "*://www.xuexi.cn/"
      ]
    },
    {
      "js": [
        "js/runtime.js"
      ],
      "matches": [
        "*://www.xuexi.cn/*"
      ]
    },
    {
      "js": [
        "js/login.js"
      ],
      "matches": [
        "*://pc.xuexi.cn/points/login.html*"
      ]
    }
  ],
  "permissions": [
    "*://*.xuexi.cn/*",
    "storage",
    "notifications"
  ]
}
